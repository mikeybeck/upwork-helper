{
  "manifest_version": 3,
  "name": "Upwork helper",
  "version": "1.0",
  "permissions": ["activeTab", "webRequest", "storage"],
  "host_permissions": [
    "*://*.upwork.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
